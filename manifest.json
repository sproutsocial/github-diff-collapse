{
	"manifest_version": 2,
	"name": "Better Pull Requests",
	"version": "1.0.0",
	"permissions": [
		"storage",
		"<all_urls>"
	],
	"options_page": "options.html",
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": [
				"jquery.js",
				"jquery.livequery.js",
				"helpers.js",
				"github.js",
				"gitlab.js",
				"bitbucket.js"
			],
			"css": [
				"github.css",
				"gitlab.css",
				"bitbucket.css"
			]
		}
	]
}